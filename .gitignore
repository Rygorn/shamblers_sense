.vs/
*.user
*.suo
*.userprefs
*.csproj.user
*.cache
*.dbmdl

# Rider IDE specific
.idea/

# MacOS files
.DS_Store

# Logs
*.log

# Temporary files
*.tmp
*.temp

# NuGet packages (if any)
packages/

# Packages folder for Unity projects (if any)
Library/
Temp/
Obj/

